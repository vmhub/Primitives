<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.seventhree.drawprimitives"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="23" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.seventhree.drawprimitives.Select"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.seventhree.drawprimitives.Line"
            android:label="@string/title_activity_line" >
        </activity>
        <activity
            android:name="com.example.seventhree.drawprimitives.Circle"
            android:label="@string/title_activity_circle" >
        </activity>
        <activity
            android:name="com.example.seventhree.drawprimitives.Picture"
            android:label="@string/title_activity_picture" >
        </activity>
        <activity
            android:name="com.example.seventhree.drawprimitives.Rectangle"
            android:label="@string/title_activity_rectangle" >
        </activity>
        <activity
            android:name="com.example.seventhree.drawprimitives.Input"
            android:label="@string/title_activity_input" >
        </activity>
    </application>

</manifest>